# Agent Foundry Terraform Variables
# Copy this file to terraform.tfvars and fill in your values

# AWS Configuration
aws_region = "us-east-1"
environment = "dev"
instance_type = "t3.small"

# Domain Configuration
domain = "foundry.ravenhelm.dev"

# SSH Key (must exist in AWS)
# Create one with: aws ec2 create-key-pair --key-name foundry-dev --query 'KeyMaterial' --output text > ~/.ssh/foundry-dev.pem
ssh_key_name = "foundry-dev"
